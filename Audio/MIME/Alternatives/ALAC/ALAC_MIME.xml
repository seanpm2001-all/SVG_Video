<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is ALAC, as not everybody will want to use OGG, and ALAC playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  ALAC formats
  .ALAC
  Supported ALAC formats
  .ALAC
  !-->
  <!-- Not supported: video/mp4, !-->
    <ALACMIMETypeTrueAudio>audio>ALAC</ALACMIMETypeTrueAudio>
  <!-- Not supported: application/ALAC !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/ALAC</isoMIMETypeA>
    </MIMETypeAudio>
</audioMIMEType>
