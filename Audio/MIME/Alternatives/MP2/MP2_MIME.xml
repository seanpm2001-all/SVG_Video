<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is MP2, as not everybody will want to use OGG, and MP2 playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  MP2 formats
  .mp2
  Supported MP2 formats
  .mp2
  !-->
  <!-- Not supported: video/mp4, !-->
    <mp2MIMETypeTrueAudio>audio>MP2</mp2MIMETypeTrueAudio>
  <!-- Not supported: application/mp2 !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/mp2</isoMIMETypeA>
    </MIMETypeAudio>
</audioMIMEType>
