<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is MP3, as not everybody will want to use OGG, and MP3 playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  MP3 formats
  .mp3
  Supported MP3 formats
  .mp3
  !-->
  <!-- Not supported: video/mp4, !-->
    <mp3MIMETypeTrueAudio>audio>MP3</mp3MIMETypeTrueAudio>
  <!-- Not supported: application/mp3 !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/mp3</isoMIMETypeA>
    </MIMETypeAudio>
</audioMIMEType>
