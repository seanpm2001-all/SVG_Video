<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is FLAC, as not everybody will want to use OGG, and FLAC playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  FLAC formats
  .FLAC
  Supported FLAC formats
  .FLAC
  !-->
  <!-- Not supported: video/mp4, !-->
    <FLACMIMETypeTrueAudio>audio>FLAC</FLACMIMETypeTrueAudio>
  <!-- Not supported: application/FLAC !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/FLAC</isoMIMETypeA>
    </MIMETypeAudio>
</audioMIMEType>
