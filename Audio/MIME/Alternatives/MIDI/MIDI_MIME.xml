<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is MIDI, as not everybody will want to use OGG, and MIDI playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  MIDI formats
  .midi .mid
  Supported MIDI formats
  .midi .mid
  !-->
  <!-- Not supported: video/midi, !-->
    <midiMIMETypeTrueAudio>audio>MIDI</midiMIMETypeTrueAudio>
    <altMidiMIMETypeTrueAudio>audio/MID</altMidiMIMETypeTrueAudio>
  <!-- Not supported: application/midi !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/midi</isoMIMETypeA>
    <isoMIMETypeB>audio/mid</isoMIMETypeB>
    </MIMETypeAudio>
</audioMIMEType>
