<!-- Start of script !-->
<!-- This alternative MIME type for SVG Video is WAV, as not everybody will want to use OGG, and WAV playback may be needed !-->
<audioMIMEType>
  <!-- The default audio MIME type for SVG video !-->
  <webMIMEType> <!-- For websites and web applications !-->
  <!--
  WAV formats
  .wav
  Supported WAV formats
  .wav
  !-->
  <!-- Not supported: video/wav, !-->
    <wavMIMETypeTrueAudio>audio>WAV</wavMIMETypeTrueAudio>
  <!-- Not supported: application/wav !-->
  </webMIMEType>
  <MIMETypeAudio> <!-- For non-web applications !-->
    <isoMIMETypeA>audio/WAV</isoMIMETypeA>
    </MIMETypeAudio>
</audioMIMEType>
